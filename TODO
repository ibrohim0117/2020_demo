CRETAE TABLE IF NOT EXISTS users(
    id INT,
    name VARCHAR(25)
)

INSERT INTO users(id, name) VALUES(1, 'Mark')

SELECT * from users

UPDATE users SET name='B' WHERE id=3

DELETE from users WHERE id=3





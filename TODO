class Shaxs:

    def __init__(self, ism, familya, yosh):
        self.ism = ism
        self.familya = familya
        self.yosh = yosh

    def salom():
        return self.ism


class Inson(Shaxs):

    def salom():
        return self.yosh

shaxs1 = Shaxs('Mark', 'Alijonov', 43)
print(shaxs1.salom())


inson1 = Inson('K', 'N', 34)
print(inson1.salom())



